#include <iostream>

int main() {
    int T;
    std::cin >> T; // Number of test cases
    
    for (int i = 0; i < T; ++i) {
        int N, X;
        std::cin >> N >> X; // Required donations and collected donations
        
        int remainingDonations = N - X;
        std::cout << remainingDonations << std::endl; // Output the remaining donations
    }
    
    return 0;
}
